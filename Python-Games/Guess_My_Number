#in this game, the user plays guess the number game
#import the random is used.

import random
tries = 5

computer_guess = random.randint(1, 100)

for mytries in range(tries):
    user_guess = int(input("Enter a number from 1 to 100 "))
    if user_guess > computer_guess:
        print("Lower", computer_guess)
    elif user_guess < computer_guess:
        print("Higher", computer_guess)
    else:
        print "Correct number"
else:
    print("You failed to guess the number")
